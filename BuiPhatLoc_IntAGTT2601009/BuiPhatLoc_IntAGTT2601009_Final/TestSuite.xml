<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Login Test Suite" parallel="tests" thread-count="1">
	<parameter name="browser" value="chrome"/>
	<test name="Test Suite - Chrome">
		<parameter name="browser" value="chrome"/>
		<classes>
			<class name="AutomationExercise.SeleniumExam"/>
		</classes>
	</test>
	
	<test name="Test Suite - Firefox">
		<parameter name="browser" value="firefox"/>
		<classes>
			<class name="AutomationExercise.SeleniumExam"/>
		</classes>
	</test>
</suite>